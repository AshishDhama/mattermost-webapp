{
  "name": "@mattermost/compass-ui",
  "version": "1.0.0",
  "description": "shared component library for Mattermost",
  "main": "./src/index.ts",
  "keywords": [
    "storybook",
    "react",
    "typescript",
    "mattermost"
  ],
  "scripts": {
    "test": "test",
    "storybook": "start-storybook -p 6006",
